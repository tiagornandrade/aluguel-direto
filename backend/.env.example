# Database. Use file:./dev.db for SQLite (dev) or postgresql://... for production.
DATABASE_URL="file:./dev.db"

# Server
PORT=4000

# CORS (comma-separated origins; * for dev only)
CORS_ORIGINS="http://localhost:3000"

# Internal API key (must match frontend .env.local). Used by BFF/proxy for X-Api-Key.
# Generate with: openssl rand -base64 32
INTERNAL_API_KEY=
